{
  "version": 2,
  "buildCommand": "cd clerk-javascript && npm install && npm run build",
  "outputDirectory": "clerk-javascript/dist",
  "routes": [
    {
      "src": "/api/user/(.*)",
      "dest": "/api/user-api/$1"
    },
    {
      "src": "/api/assessment/(.*)",
      "dest": "/api/assessment-api/$1"
    },
    {
      "src": "/api/progress/(.*)",
      "dest": "/api/progress-api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
