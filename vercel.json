{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "clerk-javascript/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "clerk-javascript/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "clerk-javascript/dist/$1"
    }
  ]
}
